{
  "name": "google-review-ai-reply",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "yarn format:check && next build",
    "start": "next start",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "knip": "knip",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push --verbose",
    "db:migrate": "drizzle-kit migrate --verbose",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:triggers": "dotenv -e .env -- tsx scripts/setup-trigger.ts",
    "webhook:trigger": "tsx scripts/trigger-review-webhook.ts",
    "cron:trigger": "tsx scripts/trigger-weekly-summary.ts",
    "comments:remove": "tsx scripts/remove-comments.ts",
    "pubsub:update": "bash scripts/update-pubsub-subscription.sh",
    "email:dev": "npx react-email dev --dir lib/emails",
    "email:export": "npx react-email export --dir lib/emails",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook"
  },
